{
  "containerDefinitions": [
    {
      "name": "exercises",
      "image": "%s.dkr.ecr.us-east-1.amazonaws.com/coding-app-exercises:staging",
      "essential": true,
      "memoryReservation": 300,
      "links": [
      	"exercises-db"
      ],
      "portMappings": [
        {
          "hostPort": 0,
          "protocol": "tcp",
          "containerPort": 5000
        }
      ],
      "logConfiguration": {
      	"logDriver": "awslogs",
      	"options": {
      	  "awslogs-group": "coding-app-exercises-stage",
      	  "awslogs-region": "us-east-1"
      	}
      },
      "environment": [
      	{
          "name": "APP_SETTINGS",
          "value": "project.config.StagingConfig"
        },
        {
          "name": "DATABASE_TEST_URL",
          "value": "postgres://postgres:postgres@exercises-db:5432/exercises_test"
        },
        {
          "name": "DATABASE_URL",
          "value": "postgres://postgres:postgres@exercises-db:5432/exercises_stage"
        },
        {
          "name": "SECRET_KEY",
          "value": "123456"
        }
      ]
    },
    {
      "name": "exercises-db",
      "image": "%s.dkr.ecr.us-east-1.amazonaws.com/coding-app-exercises_db:staging",
      "essential": true,
      "memoryReservation": 300,
      "portMappings": [
        {
          "hostPort": 0,
          "protocol": "tcp",
          "containerPort": 5432
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "coding-app-exercises_db-stage",
          "awslogs-region": "us-east-1"
        }
      },
      "environment": [
        {
          "name": "POSTGRES_PASSWORD",
          "value": "postgres"
        },
        {
          "name": "POSTGRES_USER",
          "value": "postgres"
        }
      ]
    }
  ],
  "family": "coding-app-exercises-stage"
}