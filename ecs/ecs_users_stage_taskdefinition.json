{
  "containerDefinitions": [
    {
      "name": "users",
      "image": "599583304279.dkr.ecr.us-east-1.amazonaws.com/coding-app-users:staging",
      "essential": true,
      "memoryReservation": 300,
      "links": [
      	"users-db"
      ],
      "portMappings": [
        {
          "hostPort": 0,
          "protocol": "tcp",
          "containerPort": 5000
        }
      ],
      "logConfiguration": {
      	"logDriver": "awslogs",
      	"options": {
      	  "awslogs-group": "coding-app-users-stage",
      	  "awslogs-region": "us-east-1"
      	}
      },
      "environment": [
      	{
          "name": "APP_SETTINGS",
          "value": "project.config.StagingConfig"
        },
        {
          "name": "DATABASE_TEST_URL",
          "value": "postgres://postgres:postgres@users-db:5432/users_test"
        },
        {
          "name": "DATABASE_URL",
          "value": "postgres://postgres:postgres@users-db:5432/users_stage"
        },
        {
          "name": "SECRET_KEY",
          "value": "123456"
        }
      ]
    },
    {
      "name": "users-db",
      "image": "599583304279.dkr.ecr.us-east-1.amazonaws.com/coding-app-users_db:staging",
      "essential": true,
      "memoryReservation": 300,
      "portMappings": [
        {
          "hostPort": 0,
          "protocol": "tcp",
          "containerPort": 5432
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "coding-app-users_db-stage",
          "awslogs-region": "us-east-1"
        }
      },
      "environment": [
        {
          "name": "POSTGRES_PASSWORD",
          "value": "postgres"
        },
        {
          "name": "POSTGRES_USER",
          "value": "postgres"
        }
      ]
    }
  ],
  "family": "coding-app-users-stage",
}